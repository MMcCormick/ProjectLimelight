<a class="tlink" href="/<%= @influencer_topic.get('topic').get('slug') %>" data-id='<%= @influencer_topic.get('topic').get('id') %>'>
  <img src='<%= @influencer_topic.get('topic').get('images').square.small %>' />
  <h4>
    <%= @influencer_topic.get('topic').get('name') %>
  </h4>
</a>
<div class="stats">
  Ranked <span class="rank">#<%= @influencer_topic.get('rank') %></span>
  with <span class="influence"><%= @influencer_topic.get('influence') %></span> pts
</div>
<% content_for :page_header do %>
  <h1><%= @user.username %>'s contributions</h1>
<% end %>

<% if current_user != @user %>
  <% content_for :sidebar_right do %>
    <%= render_cell(:user, :sidebar_right, current_user, @user) %>
  <% end %>
<% end %>

<% content_for :page_header do %>
  <%= render 'core_objects/feed_filters' %>
<% end %>

<%= render "core_objects/feed", :core_objects => @core_objects, :more_path => @more_path %>

<div class='inner'>
  <ul class='unstyled'>
    <li class='on all'>
      <% if LL.App.current_user && LL.App.current_user.get('id') == @model.get('id'): %>
        <a href="/activity">All Posts</a>
      <% else: %>
        <a href="<%= @model.get('url') %>">All Posts</a>
      <% end %>
    </li>
  </ul>
</div>
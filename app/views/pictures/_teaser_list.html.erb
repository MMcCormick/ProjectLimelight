<% img = default_image_url(object, 75, 75, 'fit') %>
<% original = default_image_url(object, 0, 0, :original) %>
<% image = image_tag img, 'data-o' => '{"url":"'+original.image_url+'","w":"'+original.width.to_s+'","h":"'+original.height.to_s+'"}' if img %>
<%= render 'core_objects/feed_teaser_image', :image => image %>
<h1>New news</h1>

<%= form_for(@talk) do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <div class="field">
    <%= f.label :content %><br />
    <%= f.text_field :content %>
  </div>
  <div class="actions">
    <%= f.submit "Post Talk" %>
  </div>
<% end %>

<%= link_to 'Back', :back %>

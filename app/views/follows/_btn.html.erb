<% unless signed_in? && current_user.id == target.id %>
  <button class="btn-tog <%= !signed_in? || !current_user.is_following?(target) ? 'followB' : 'unfollowB' %> fol_<%= target.id %>">
    <div class="default" data-url="<%= create_follow_path %>" data-d='{"id":"<%= target.id %>", "type":"<%= target.class.name %>"}' data-m="post">
      Follow
    </div>
    <div class="triggered" data-url="<%= destroy_follow_path %>" data-d='{"id":"<%= target.id %>", "type":"<%= target.class.name %>"}' data-m="delete">
      Unfollow
    </div>
  </button>
<% end %>
<a href="<%= if LL.App.current_user && LL.App.current_user.get('id') == @model.get('id') then "/activity#{@item.get('topic').get('url')}" else "#{@model.get('url')}#{@item.get('topic').get('url')}" %>">
  <img src='<%= @image %>' />
  <div class='text'>
    <div class='subtitle'><%= @item.get('count') %> Post<%= if @item.get('count') != 1 then 's' else '' %></div>
    <div class='name'><%= @item.get('topic').get('name') %></div>
  </div>
</a>
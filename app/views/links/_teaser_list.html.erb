<% img = default_image_url(object, 100, 100, 'fit', false, true) %>
<% original = default_image_url(object, 0, 0, :original) if img %>
<% image = image_tag img.image_url, 'data-o' => '{"url":"'+original.image_url+'","w":"'+original.width.to_s+'","h":"'+original.height.to_s+'"}' if img %>
<%= render 'core_objects/feed_teaser_image', :image => image if img %>
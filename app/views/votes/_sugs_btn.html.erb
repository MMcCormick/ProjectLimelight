<div class="scoreC">
  <button class="btn-tog <%= !signed_in? || !object.voter?(current_user.id, 1) ? 'voteB' : 'unvoteB' %> up v_<%= object.id %>" title='<%= !signed_in? || !object.voter?(current_user.id, 1) ? "this is correct" : "undo +" %>'>
    <div class="default" data-url="<%= create_vote_path %>" data-d='{"id":"<%= object.id %>","a":"1", "type":"<%= object.class.name %>"}' data-m="post">
      This is Correct
    </div>
    <div class="triggered" data-url="<%= destroy_vote_path %>" data-d='{"id":"<%= object.id %>", "type":"<%= object.class.name %>"}' data-m="delete">
      This is Correct
    </div>
  </button>
  <button class="btn-tog <%= !signed_in? || !object.voter?(current_user.id, -1) ? 'voteB' : 'unvoteB' %> down v_<%= object.id %>" title='<%= !signed_in? || !object.voter?(current_user.id, -1) ? "this is incorrect" : "undo -" %>'>
    <div class="default" data-url="<%= create_vote_path %>" data-d='{"id":"<%= object.id %>","a":"-1", "type":"<%= object.class.name %>"}' data-m="post">
      This is Incorrect
    </div>
    <div class="triggered" data-url="<%= destroy_vote_path %>" data-d='{"id":"<%= object.id %>", "type":"<%= object.class.name %>"}' data-m="delete">
      This is Incorrect
    </div>
  </button>
</div>